{
  "name": "@northware/ui",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "tsc-check": "tsc --noEmit",
    "format": "ultracite format",
    "lint": "ultracite lint",
    "generate:icons": "svgr --config-file svgr.config.cjs --out-dir ./src/icons/generated --ignore-existing -- ./src/icons/raw"
  },
  "exports": {
    "./css": "./src/globals.css",
    "./lib/*": "./src/lib/*.ts",
    "./components/*": "./src/components/*.tsx",
    "./icons/lucide": "./src/icons/lucide.ts",
    "./icons/generated/*": "./src/icons/generated/*.tsx",
    "./hooks/*": "./src/hooks/*.ts",
    "./postcss.config": "./postcss.config.mjs"
  },
  "dependencies": {
    "@clerk/elements": "^0.23.9",
    "@hookform/resolvers": "^5.0.1",
    "@northware/auth": "workspace:*",
    "@northware/database": "workspace:",
    "@northware/service-config": "workspace:",
    "@radix-ui/react-accordion": "^1.2.3",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-navigation-menu": "^1.2.5",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-switch": "^1.1.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.41.0",
    "lucide-react": "^0.487.0",
    "next": "15.2.4",
    "next-themes": "^0.4.6",
    "react-hook-form": "^7.55.0",
    "tailwind-merge": "^3.2.0",
    "tw-animate-css": "^1.2.5",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@northware/tsconfig": "workspace:*",
    "@svgr/cli": "^8.1.0",
    "@tailwindcss/postcss": "^4.1.3",
    "@types/react": "^19.1.0",
    "tailwindcss": "^4.1.3"
  }
}
